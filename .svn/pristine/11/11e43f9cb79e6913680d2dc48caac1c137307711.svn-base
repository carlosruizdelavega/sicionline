/*estilo de formulario en Modal*/
.formModal{
	font-size:12px;
}
.formModal input[type="text"]{
	font-size:13px;
	padding: 2px;
	width: 160px;
	margin-top:-1px;
}
.formModal fieldset legend{
	font-size:15px;
	font-weight: bold;
	margin-top: -10px;
	margin-bottom: 10px;
	line-height: 30px;
	padding:0px;
 	color: #369;
}
.formModal .subtitle{
	font-size:17px;
	font-weight: normal;
}
.formModal span{
	font-weight: bold;
}
.formModal .row-fluid{
 	margin-left:8px;
}


