/*contenedor de contenido*/
.backContent{
    background-color: #F4F4F4;
    border: 6px solid #FFFFFF;
    border-radius: 10px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05) inset;
    min-height: 20px;
    padding: 20px;
}
.backContentWhite{
    background-color: #FFFFFF;
    border: 1px solid #cccccc;
    border-radius: 10px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05) inset;
    min-height: 20px;
    padding: 10px;
}
.backContentDialog{
    background-color: #FFFFFF;
    border: 1px solid #cccccc;
    border-radius: 10px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05) inset;
   	min-height: 20px;
    padding: 10px;
    text-align: center;
	margin:0 auto;
    
}

/*contenedor paginado*/
.backPaging{
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 14px;
	font-weight: bold;
	color: #444444;
	width:auto;
	height: 50px;
    margin-bottom:10px;
    margin-top:10px;
    background-color: #efefef;
    border-radius: 6px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05) inset;
    padding:0px;
}

/*paginas*/
.backPaging ul {
	float: left;
	margin-top: 10px;
	width: auto;
}
.backPaging ul li {
    display: inline;
	float: left;
	width: 32px;
	height: 26px;
    margin: 0px;
    padding: 0px;
}
.backPaging ul li a {
  display: inline-block;
  width: 30px;
  height: 24px;
  border: 1px solid #ddd;
  padding: 0px;
  padding-top: 5px;
  background-color: #fff;  
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  text-align: center;
  vertical-align: middle;
}
.backPaging ul li a:hover {
  text-decoration: none;
  background-color: #f5f5f5;
}

/*busqueda*/
.backPaging .short{
	float: left;
	margin-top: 14px;
	margin-left: 40px;
	width:auto;
}
.backPaging .short input[type="text"]{
	float: left;
	width: 30px;
	padding: 2px;	
}
.backPaging .short .btn{
	float: left;
	margin-left: 2px;
	width: 35px;
	height: 25px;
}

/*info*/
.backPaging span{
	float: left;
	margin-top:18px;
    margin-left: 4px;
}

/*ajusta estilo de tabla (grilla de datos)*/
.table th{
	text-align: center;
	vertical-align: middle;
}
.table thead{
    background-color: #fafafa;
    background-image: linear-gradient(to bottom, #fafafa, #EAEAEA);
	color: #000000;
}

.cellt td{
border: 2px solid #ffffff;
background-color: #efefef;
font-size:11px;
padding-left: 5px;
}

.cellh td{
border: 2px solid #efefef;

font-size:11px;
padding-left: 5px;
}

.cellg td{
border: 2px solid #ccc;
font-size:11px;
padding-left: 5px;
}

.celli td{
font-size:11px;
padding: 1px;	
border: 2px solid #ccc;	
}

.cellts td{

font-size:11px;
padding: 1px;

}

.cellv td{

